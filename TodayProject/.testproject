    <environment name="QA">
      <summary/>
    </environment>
    <environment name="UAT">
      <summary/>
    </environment>
  </environments>
  <variables>
    <testVariable name="Animal">
      <valueType class="textType">
        <constraints>
          <valueConstraint name="minLength">0</valueConstraint>
          <valueConstraint name="maxLength">9999</valueConstraint>
          <valueConstraint name="regex"/>
        </constraints>
      </valueType>
      <summary/>
      <values>
        <testVariableValue>
          <value class="value" valueClass="string">Dog</value>
        </testVariableValue>
        <testVariableValue envName="Dev">
          <valueType class="textType">
            <constraints>
              <valueConstraint name="minLength">0</valueConstraint>
              <valueConstraint name="maxLength">9999</valueConstraint>
              <valueConstraint name="regex"/>
            </constraints>
          </valueType>
          <value class="value" valueClass="string">Camel</value>
        </testVariableValue>
        <testVariableValue envName="QA">
          <valueType class="textType">
            <constraints>
              <valueConstraint name="minLength">0</valueConstraint>
              <valueConstraint name="maxLength">9999</valueConstraint>
              <valueConstraint name="regex"/>
            </constraints>
          </valueType>
          <value class="value" valueClass="string">Cat</value>
        </testVariableValue>
        <testVariableValue envName="UAT">
          <valueType class="textType">
            <constraints>
              <valueConstraint name="minLength">0</valueConstraint>
              <valueConstraint name="maxLength">9999</valueConstraint>
              <valueConstraint name="regex"/>
            </constraints>
          </valueType>
          <value class="value" valueClass="string">Penguin</value>
        </testVariableValue>
      </values>
    </testVariable>
    <testVariable name="_chromeOptions">
      <valueType class="textType">
        <constraints>
          <valueConstraint name="minLength">0</valueConstraint>
          <valueConstraint name="maxLength">9999</valueConstraint>
          <valueConstraint name="regex"/>
        </constraints>
      </valueType>
      <summary/>
      <values>
        <testVariableValue>
          <value class="value" valueClass="string">--headless</value>
        </testVariableValue>
      </values>
    </testVariable>
  </variables>
